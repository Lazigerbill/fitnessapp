<%= form_for @instructor do |f| %>
	<%= image_tag @instructor.avatar.url(:thumb) %>
	<%= f.file_field :avatar %> 
	<%= f.label :first_name %>
	<%= f.text_field :first_name %>
	<%= f.label :last_name %> 
	<%= f.text_field :last_name %> 
	<%= f.label :phone %> 
	<%= f.text_field :phone %> 
	<%= f.label :street_address %> 
	<%= f.text_field :street_address %> 
	<%= f.label :city %> 
	<%= f.text_field :city %> 
	<%= f.label :postal_code %> 
	<%= f.text_field :postal_code %> 
	<%= f.label :expertise %> 
	<%= f.text_field :expertise %> 
	<br>
	<%= f.submit %>    <%= link_to "back", root_path %>
<% end %>